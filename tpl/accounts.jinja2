{% extends "template.jinja2" %}

{%  block body %}
<h1>Accounts</h1>
{{ paginate(page) }}
<table>
{% for account in accounts %}
<tr>
    <td>{{ account_url(account) }}</td>
    <td>{{ account.contact }}</td>
    <td>{{ account.status.name }}</td>
</tr>
{% endfor %}
</table>
{{ paginate(page) }}
{% endblock %}
